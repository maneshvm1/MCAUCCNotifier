        <activity android:name=".ListPhotosActivity" />
        <activity android:name=".ListSuggestionsActivity" />
        <activity android:name=".NewSuggestionActivity" />
        <activity android:name=".ViewNoticeActivity" />
        <activity android:name=".NewNoticeActivity" />
        <activity android:name=".NewDocumentActivity" />
        <activity android:name=".ViewPhotoActivity" />
        <activity android:name=".NewReminderActivity" />
